msgid  "Record an audio file -- usage:\n"
       "\t%s [-af] [-v vol] [-b bal] [-m monvol]\n"
       "\t%.*s [-p mic|line|cd|aux1|aux2|spdif]\n"
       "\t%.*s [-c channels] [-s rate] [-e encoding]\n"
       "\t%.*s [-t time] [-i info] [-d dev] [-T au|wav|aif[f]] [file]\n"
       "where:\n"
       "\t-a\tAppend to output file\n"
       "\t-f\tIgnore sample rate differences on append\n"
       "\t-v\tSet record volume (0 - %d)\n"
       "\t-b\tSet record balance (%d=left, %d=center, %d=right)\n"
       "\t-m\tSet monitor volume (0 - %d)\n"
       "\t-p\tSpecify input port\n"
       "\t-c\tSpecify number of channels to record\n"
       "\t-s\tSpecify rate in samples per second\n"
       "\t-e\tSpecify encoding (ulaw | alaw | [u]linear | linear8 )\n"
       "\t-t\tSpecify record time (hh:mm:ss.dd)\n"
       "\t-i\tSpecify a file header information string\n"
       "\t-d\tSpecify audio device (default: /dev/audio)\n"
       "\t-T\tSpecify the audio file type (default: au)\n"
       "\tfile\tRecord to named file\n"
       "\t\tIf no file specified, write to stdout\n"
       "\t\tDefault audio encoding is ulaw, 8khz, mono\n"
       "\t\tIf -t is not specified, record until ^C\n"
msgstr "Audiodatei aufzeichnen -- Syntax:\n"
       "\t%s [-af] [-v Lautst] [-b Bal] [-m MonLautst]\n"
       "\t%.*s [-p mic|line|cd|aux1|aux2|spdif]\n"
       "\t%.*s [-c Kanäle] [-s Rate] [-e Kodierung]\n"
       "\t%.*s [-t Dauer] [-i Info] [-d Gerät] [-T au|wav|aif[f]] [Datei]\n"
       "wobei:\n"
       "\t-a\tAn Ausgabedatei anhängen\n"
       "\t-f\tUnterschiede in der Abtastrate beim Anhängen ignorieren\n"
       "\t-v\tAufzeichnungslautstärke einstellen (0 - %d)\n"
       "\t-b\tAufzeichnungsbalance einstellen (%d=links, %d=Mitte, %d=rechts)\n"
       "\t-m\tMonitorlautstärke einstellen (0 - %d)\n"
       "\t-p\tEingangsport angeben\n"
       "\t-c\tAnzahl der aufzuzeichnenden Kanäle angeben\n"
       "\t-s\tAbtastrate in Abtastungen pro Sekunde angeben\n"
       "\t-e\tKodierung angeben (ulaw | alaw | [u]linear | linear8 )\n"
       "\t-t\tAufzeichnungsdauer angeben (hh:mm:ss.dd)\n"
       "\t-i\tInformations-Header für Datei angeben\n"
       "\t-d\tAudiogerät angeben (Standardeinstellung: /dev/audio)\n"
       "\t-T\tAudiodateityp angeben (Standardeinstellung: au)\n"
       "\tDatei\tIn angegebene Datei aufzeichnen\n"
       "\t\tFalls keine Datei angegeben ist, nach stdout schreiben\n"
       "\t\tStandard-Audiokodierung ist ulaw, 8kHz, Mono\n"
       "\t\tFalls -t nicht angegeben ist, bis zur Eingabe von ^C aufzeichnen\n"
